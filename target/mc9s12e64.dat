info single chip MC9S12E64 MCU

mcu MC9S12E64
family HCS12
ram_size 0x1000
eeprom_module NONE
flash_module FTS128K1
flash_size 0x10000
ppage_base 0x3c
ppage_count 4
ppage_default 0x3c

# INITRM = 0x30
bdm_init_byte 0x0010 0x30

bdm_startup_delay 100
bdm_agent bdm.s19

# only BDM flash write uses RAM agent
bdm_flash_write agent

lrae_agent lrae.s19
lrae_size 0x0200
